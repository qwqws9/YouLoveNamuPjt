		// 현재 날짜 및 시간
		function currentTime() {
			var day = new Date();
			
			return day.getFullYear() + '년 ' + (day.getMonth()+1) + '월 ' + day.getDate() + '일 ' + day.getHours() + ':' + day.getMinutes();
		}
		
